{
  "sources": [{
    "type": "csv",
    "location": "{{inputs.appsflyer.location}}/{{product_id}}/{{event_date}}/installs_report"
  }],
  "targets": [{
    "location": "{{location(datastore)}}/{{product_id}}/devices/installs/{{event_date}}",
    "schema": [
      { "name": "product_id", "transform": ["set_product_id"] },
      { "name": "event_time", "transform": ["to_date"], "alias": "event_date" },
      { "name": "appsflyer_id", "alias": "install_id" },
      { "name": "install_time", "transform": ["cast(timestamp)"], "alias": "event_time" },
      { "name": "media_source" },
      { "name": "campaign" },
      { "name": "af_c_id", "alias": "campaign_id" },
      { "name": "af_cost_model", "alias": "cost_model" },
      { "name": "af_cost_value", "transform": ["cast(double)"], "alias": "cost_value" },
      { "name": "af_cost_currency", "alias": "cost_currency" },
      { "name": "country_code" },
      { "name": "platform" }
    ]
  }]
}