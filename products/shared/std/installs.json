{
  "sources": [{
    "type": "csv",
    "options": {
      "header": "true"
    },
    "location": "{{inputs.appsflyer.location}}/install_reports/event_date={{event_date}}"
  }],
  "targets": [{
    "location": "{{datastore.location}}/devices/installs/event_date={{event_date}}",
    "columns": [
      { "name": "product_id", "transform": ["set_product_id"] },
      { "name": "event_date", "transform": ["col(event_time)", "to_date"] },
      { "name": "appsflyer_id", "alias": "install_id" },
      { "name": "event_time", "transform": ["col(install_time)", "cast(timestamp)"] },
      { "name": "media_source" },
      { "name": "campaign" },
      { "name": "af_c_id", "alias": "campaign_id" },
      { "name": "af_cost_model", "alias": "cost_model" },
      { "name": "af_cost_value", "transform": ["cast(double)"], "alias": "cost_value" },
      { "name": "af_cost_currency", "alias": "cost_currency" },
      { "name": "country_code" },
      { "name": "platform" }
    ]
  }]
}