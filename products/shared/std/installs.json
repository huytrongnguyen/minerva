{
  "sources": [{
    "type": "csv",
    "options": {
      "header": "true"
    },
    "location": "{{inputs.appsflyer.location}}/install_reports/event_date={{event_date}}"
  }],
  "targets": [{
    "location": "{{datastore.location}}/devices/installs",
    "partition_by": ["event_date"],
    "columns": [
      { "name": "product_id", "funcs": ["set_product_id"] },
      { "name": "event_time", "funcs": ["to_date"], "alias": "event_date" },
      { "name": "appsflyer_id", "alias": "install_id" },
      { "name": "install_time", "funcs": ["cast(timestamp)"] },
      { "name": "media_source" },
      { "name": "campaign" },
      { "name": "af_c_id", "alias": "campaign_id" },
      { "name": "af_cost_model", "alias": "cost_model" },
      { "name": "af_cost_value", "funcs": ["cast(double)"], "alias": "cost_value" },
      { "name": "af_cost_currency", "alias": "cost_currency" },
      { "name": "country_code" },
      { "name": "platform" }
    ]
  }]
}