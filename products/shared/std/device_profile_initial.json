{
  "sources": [{
    "location": "{{datastore.location}}/devices/installs/event_date={{event_date}}"
  }],
  "targets": [{
    "location": "{{datastore.location}}/device_profile",
    "columns": [
      { "name": "product_id" },
      { "name": "install_id" },
      { "name": "media_source" },
      { "name": "campaign" },
      { "name": "campaign_id" },
      { "name": "country_code" },
      { "name": "platform" },
      { "name": "install_time" }
    ],
    "query": ["drop_duplicates(product_id,install_id)"]
  }]
}